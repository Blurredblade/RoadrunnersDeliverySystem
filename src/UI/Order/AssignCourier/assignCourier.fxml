<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="206.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Order.AssignCourier.AssignCourierController">
   <children>
      <ComboBox fx:id="courierSelect" layoutX="279.0" layoutY="95.0" onAction="#selectionChanged" prefWidth="150.0" promptText="Select a Courier" />
      <Label layoutX="171.0" layoutY="99.0" text="Courier:" />
      <Label fx:id="title" alignment="CENTER" layoutX="49.0" layoutY="34.0" prefHeight="32.0" prefWidth="502.0" text="Choose a Courier for Order">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button layoutX="196.0" layoutY="147.0" mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="66.0" text="Save" />
      <Button layoutX="339.0" layoutY="147.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="66.0" text="Cancel" />
   </children>
</AnchorPane>
