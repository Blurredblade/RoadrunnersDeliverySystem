<?xml version="1.0" encoding="UTF-8"?>

<!-- Order Tab -->
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Order.OrderController">
    <children>
        <Button fx:id="newOrderBtn" layoutX="41.0" layoutY="58.0" mnemonicParsing="false" text="New Order"/>
        <Label layoutX="442.0" layoutY="14.0" text="Orders">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="304.0" layoutY="62.0" text="Filter by Status:"/>
        <ChoiceBox layoutX="394.0" layoutY="58.0" prefWidth="150.0"/>
        <TableView fx:id="orderTable" layoutX="21.0" layoutY="92.0" prefHeight="322.0" prefWidth="879.0">
            <columns>
                <TableColumn prefWidth="34.0" text="[]"/>
                <TableColumn fx:id="colOrderNum" prefWidth="54.0" text="Order #"/>
                <TableColumn fx:id="colDeliveryCust" prefWidth="255.0" text="Delivery Customer"/>
                <TableColumn fx:id="colPickUpCust" prefWidth="171.0" text="Pickup Customer"/>
                <TableColumn fx:id="colPickUpTime" minWidth="0.0" prefWidth="100.0" text="Pickup Time"/>
                <TableColumn fx:id="colPickCourier" prefWidth="82.0" text="Courier"/>
                <TableColumn fx:id="colEstDepart" minWidth="6.0" prefWidth="180.0" text="Estimated Departure"/>
            </columns>
        </TableView>
        <Button fx:id="orderInfoBtn" layoutX="21.0" layoutY="436.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Order Info"/>
        <Button fx:id="printRouteBtn" layoutX="21.0" layoutY="473.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Print Route"/>
        <Button fx:id="cancelOrderBtn" layoutX="21.0" layoutY="510.0" mnemonicParsing="false" text="Cancel Order"/>
        <Button fx:id="assignCourierBtn" layoutX="126.0" layoutY="436.0" mnemonicParsing="false" text="Assign Courier"/>
        <Button fx:id="recordTimesBtn" layoutX="126.0" layoutY="473.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="94.0" text="Record Times"/>
        <Button fx:id="logoutButton" layoutX="843.0" layoutY="601.0" mnemonicParsing="false" text="Logout"/>
        <ScrollBar layoutX="900.0" layoutY="92.0" orientation="VERTICAL" prefHeight="324.0" prefWidth="14.0"/>
        <ScrollBar layoutX="21.0" layoutY="416.0" prefHeight="14.0" prefWidth="879.0"/>
    </children>
</AnchorPane>

