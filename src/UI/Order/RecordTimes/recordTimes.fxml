<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="197.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Order.RecordTimes.RecordTimesController">
   <children>
      <Label layoutX="214.0" layoutY="35.0" prefHeight="17.0" prefWidth="226.0" text="Record Times for Order" />
      <HBox layoutX="120.0" layoutY="147.0">
         <children>
            <Button fx:id="departureButton" disable="true" mnemonicParsing="false" onAction="#departure" text="Departure">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="pickupButton" disable="true" mnemonicParsing="false" onAction="#pickup" text="Pickup">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="deliveryButton" disable="true" mnemonicParsing="false" onAction="#delivery" text="Delivery">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label layoutX="172.0" layoutY="82.0" text="Time:" />
      <TextField fx:id="time" disable="true" layoutX="260.0" layoutY="78.0" />
      <CheckBox fx:id="currTime" layoutX="204.0" layoutY="114.0" mnemonicParsing="false" onAction="#currTimeCheck" selected="true" text="Use Current Time" />
   </children>
</AnchorPane>
