<?xml version="1.0" encoding="UTF-8"?>

<!-- Business Reports Generation UI -->
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<StackPane xmlns:fx="http://javafx.com/fxml" prefWidth="231.0" fx:controller="settings.BusinessReportUIController">
    <StackPane>
        <VBox>
            <GridPane>
                <!-- Business Report Generation Screen -->
                <Label text="This is the Business Report Generation UI, Nothing is added here yet." GridPane.columnIndex="0" GridPane.rowIndex="0"/>

                <!-- UI Constraints -->
                <rowConstraints>
                    <RowConstraints vgrow="SOMETIMES" minHeight="10.0" prefHeight="30.0"/>
                    <RowConstraints vgrow="SOMETIMES" minHeight="10.0" prefHeight="30.0"/>
                </rowConstraints>

                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="300" minWidth="10.0" prefWidth="200.0"/>
                </columnConstraints>
            </GridPane>

            <StackPane>
                <!-- Log Out Button -->
                <Button fx:id="logoutButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Logout" StackPane.alignment="CENTER_RIGHT"/>
            </StackPane>
        </VBox>
    </StackPane>

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</StackPane>